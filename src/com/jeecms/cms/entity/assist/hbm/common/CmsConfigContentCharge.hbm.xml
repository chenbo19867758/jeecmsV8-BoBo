<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.cms.entity.assist">
	<class
		name="CmsConfigContentCharge"
		table="jc_config_content_charge"
	>
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id
			name="id"
			type="integer"
			column="config_content_id"
		>
			<generator class="assigned"/>
		</id>
		<property
			name="weixinAppId"
			column="weixin_appid"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="weixinSecret"
			column="weixin_secret"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="weixinAccount"
			column="weixin_account"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="weixinPassword"
			column="weixin_password"
			type="string"
			not-null="true"
			length="255"
		/>
		
		<property
			name="alipayPartnerId"
			column="alipay_partner_id"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="alipayAccount"
			column="alipay_account"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="alipayKey"
			column="alipay_key"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="alipayAppId"
			column="alipay_appid"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="alipayPublicKey"
			column="alipay_public_key"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="alipayPrivateKey"
			column="alipay_private_key"
			type="string"
			not-null="false"
			length="1000"
		/>
		<property
			name="chargeRatio"
			column="charge_ratio"
			type="java.lang.Double"
			not-null="true"
			length="5"
		/>
		<property
			name="minDrawAmount"
			column="min_draw_amount"
			type="java.lang.Double"
			not-null="true"
			length="11"
		/>
		<property
			name="commissionTotal"
			column="commission_total"
			type="java.lang.Double"
			not-null="true"
			length="11"
		/>
		<property
			name="commissionYear"
			column="commission_year"
			type="java.lang.Double"
			not-null="true"
			length="11"
		/>
		<property
			name="commissionMonth"
			column="commission_month"
			type="java.lang.Double"
			not-null="true"
			length="11"
		/>
		<property
			name="commissionDay"
			column="commission_day"
			type="java.lang.Double"
			not-null="true"
			length="11"
		/>
		<property name="lastBuyTime" column="last_buy_time" type="timestamp" not-null="false" length="19"/>
		<property
			name="payTransferPassword"
			column="pay_transfer_password"
			type="string"
			not-null="true"
			length="100"
		/>
		<property
			name="transferApiPassword"
			column="transfer_api_password"
			type="string"
			not-null="true"
			length="100"
		/>
		<property
			name="rewardMin"
			column="reward_min"
			type="java.lang.Double"
			not-null="true"
			length="11"
		/>
		<property
			name="rewardMax"
			column="reward_max"
			type="java.lang.Double"
			not-null="true"
			length="11"
		/>
		

	</class>	
</hibernate-mapping>