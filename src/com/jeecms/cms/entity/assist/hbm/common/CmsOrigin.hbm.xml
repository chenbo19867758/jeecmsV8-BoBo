<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.cms.entity.assist">
	<class
		name="CmsOrigin"
		table="jc_origin"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="origin_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="name"
			column="origin_name"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="refCount"
			column="ref_count"
			type="integer"
			not-null="true"
			length="10"
		/>


	</class>	
</hibernate-mapping>