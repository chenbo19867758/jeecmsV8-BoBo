<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.cms.entity.assist">
	<class
		name="CmsSiteAccess"
		table="jc_site_access"
	>
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id
			name="id"
			type="integer"
			column="access_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="sessionId"
			column="session_id"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="accessTime"
			column="access_time"
			type="time"
			not-null="true"
			length="8"
		/>
		<property
			name="accessDate"
			column="access_date"
			type="date"
			not-null="true"
			length="10"
		/>
		<property
			name="ip"
			column="ip"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="area"
			column="area"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="accessSource"
			column="access_source"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="externalLink"
			column="external_link"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="engine"
			column="engine"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="entryPage"
			column="entry_page"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="lastStopPage"
			column="last_stop_page"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="visitSecond"
			column="visit_second"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="visitPageCount"
			column="visit_page_count"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="operatingSystem"
			column="operating_system"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="browser"
			column="browser"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="keyword"
			column="keyword"
			type="string"
			not-null="false"
			length="255"
		/>
		<many-to-one
			name="site"
			column="site_id"
			class="com.jeecms.core.entity.CmsSite"
			not-null="true"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>