<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.cms.entity.assist">
	<class
		name="CmsSiteAccessCount"
		table="jc_site_access_count"
	>
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id
			name="id"
			type="integer"
			column="access_count"
		>
			<generator class="identity"/>
		</id>

		<property
			name="pageCount"
			column="page_count"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="visitors"
			column="visitors"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="statisticDate"
			column="statistic_date"
			type="date"
			not-null="true"
			length="10"
		/>
		<many-to-one
			name="site"
			column="site_id"
			class="com.jeecms.core.entity.CmsSite"
			not-null="true"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>