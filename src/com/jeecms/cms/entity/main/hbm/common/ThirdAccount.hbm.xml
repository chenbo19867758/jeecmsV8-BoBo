<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.jeecms.cms.entity.main">
	<class
		name="CmsThirdAccount"
		table="jc_third_account"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="java.lang.Long"
			column="account_id"
		>
			<generator class="identity"/>
		</id>

		<property
			name="accountKey"
			column="account_key"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="username"
			column="username"
			type="string"
			not-null="true"
			length="100"
		/>
		<property
			name="source"
			column="source"
			type="string"
			not-null="true"
			length="10"
		/>


	</class>	
</hibernate-mapping>